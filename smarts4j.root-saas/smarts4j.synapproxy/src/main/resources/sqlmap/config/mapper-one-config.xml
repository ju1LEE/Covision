<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
  		<setting name="callSettersOnNulls" value="true"/> <!-- null인 컬럼도 가져오는 설정 -->
  		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

    <typeAliases>
		<typeAlias type="egovframework.baseframework.data.CoviMap" alias="cmap" />
		<typeAlias type="egovframework.baseframework.data.CoviList" alias="clist" />
    </typeAliases>
</configuration>