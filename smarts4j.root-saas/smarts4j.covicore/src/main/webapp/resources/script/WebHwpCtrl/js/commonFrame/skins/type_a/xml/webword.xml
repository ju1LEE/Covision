<webOffice>
	<frame>
		<document>
			<commandItem name="e_ruler"/>
			<panel name="edit_area">
			</panel>
			<panelGroupItem name="message_bar"/>
		</document>
		<titleBar>
			<panel>
				<panel name="file" type="title" desc="{File}">
					<commandItem name="d_save" caption=""/>
					<separator line="true"/>
					<commandItem name="d_pdf_download" caption=""/>
					<separator line="true"/>
					<button name="d_page_setup" uiCommand="show" uiValue="dialog_page_setup" desc="{PageSetup}" shortcut="{PageSetupShortcut}"/>
					<commandItem name="d_print" caption="" shortcut="{PrintShortcut}"/>
					<separator line="true"/>
					<button name="d_info" uiCommand="show" uiValue="dialog_document_info" desc="{DocumentInfo}"/>
				</panel>
				<panel name="edit" type="title" desc="{Edit}">
					<commandItem name="e_undo" shortcut="{UndoShortcut}" caption=""/>
					<commandItem name="e_redo" shortcut="{RedoShortcut}" caption=""/>
					<separator line="true"/>
					<commandItem name="e_cut" shortcut="{CutShortcut}" caption=""/>
					<commandItem name="e_copy" shortcut="{CopyShortcut}" caption=""/>
					<commandItem name="e_paste" shortcut="{PasteShortcut}" caption=""/>
					<commandItem name="e_format_copy" shortcut="{CopyFormatShortcut}" caption=""/>
					<separator line="true"/>
					<commandItem name="e_format_remove" shortcut="{RemoveFormatShortcut}" caption=""/>
					<panel type="subgroup" name="e_select" desc="{Select}">
						<commandItem name="e_menu_select"/>
					</panel>
					<separator line="true"/>
					<button name="find_replace" uiCommand="show" uiValue="dialog_find_replace" shortcut="{FindAndReplaceShortcut}" desc="{FindReplace}"/>
				</panel>
				<panel name="view" type="title" desc="{View}">
					<panel type="subgroup" name="zoom" desc="{Zoom}" shortcutArea="false">
						<commandItem name="e_menu_view_scale"/>
					</panel>
					<separator line="true"/>
					<!-- 도구 상자 -->
					<!--<panel type="subgroup" name="tool_box" icon="false" desc="{Toolbox}" shortcutArea="false">-->
					<!--<button name="toolbox_standard" uiCommand="fold" uiValue="menuBar" desc="{ToolboxStandard}" icon="false"/>-->
					<!--<button name="toolbox_formatting" uiCommand="toggle" uiValue="style_bar" desc="{ToolboxFormatting}" icon="false"/>-->
					<!--</panel>-->
					<button name="taskpane" uiCommand="fold" uiValue="sideBar" desc="{Taskpane}"/>
					<panel type="subgroup" name="document_window" icon="false" desc="{DocumentWindow}" shortcutArea="false">
						<!-- 상황 선 -->
						<!--<button name="statusbar" uiCommand="toggle" uiValue="status_bar" desc="{StatusBar}" icon="false"/>-->
						<button name="show_ruler" uiCommand="toggle" uiValue="e_ruler" desc="{HorizontalRuler}"/>
					</panel>
					<separator line="true"/>
					<button name="comment" uiCommand="toggle" uiValue="commentBar" desc="{ShowComments}" disable="true"/>
				</panel>
				<panel name="insert" type="title" desc="{Insert}">
					<button name="insert_shape" uiCommand="show" uiValue="dialog_insert_shape" desc="{Shape}"/>
					<button name="insert_image" uiCommand="show" uiValue="dialog_insert_image" desc="{Image}"/>
					<button name="insert_table" uiCommand="show" uiValue="dialog_insert_table" desc="{Table}"/>
					<commandItem name="s_insert_textbox" caption=""/>
					<button name="symbols" uiCommand="show" uiValue="dialog_insert_symbols" desc="{Symbol}" shortcut="{SymbolShortcut}"/>
					<separator line="true"/>
					<panel type="subgroup" name="note" desc="{Annotation}" icon="false">
						<commandItem name="e_foot_note" shortcut="{FootnoteShortcut}" caption=""/>
						<commandItem name="e_end_note" shortcut="{EndnoteShortcut}" caption=""/>
					</panel>
					<button name="bookmark" uiCommand="show" uiValue="dialog_insert_bookmark" shortcut="{BookmarkShortcut}" desc="{Bookmark}"/>
					<button name="hyperlink" uiCommand="show" uiValue="dialog_insert_hyperlink" shortcut="{HyperlinkShortcut}" desc="{Hyperlink}"/>
				</panel>
				<panel name="format" type="title" desc="{Format}">
					<panel type="subgroup" name="font_name" desc="{FontName}" icon="false" shortcutArea="false">
						<commandItem name="menu_font_name" icon="false" captionVisible="false"/>
					</panel>
					<commandItem name="bold" shortcut="{BoldShortcut}" caption=""/>
					<commandItem name="italic" shortcut="{ItalicShortcut}" caption=""/>
					<commandItem name="underline" shortcut="{UnderlineShortcut}" caption=""/>
					<commandItem name="strikethrough" caption=""/>
					<commandItem name="text_script" captionVisible="false"/>
					<separator line="true"/>
					<panel type="subgroup" name="p_align" desc="{Alignment}">
						<commandItem name="p_align" captionVisible="false"/>
					</panel>
					<panel type="subgroup" name="line_spacing" desc="{Lineheight}" shortcutArea="false">
						<commandItem name="p_menu_line_spacing" icon="false" captionVisible="false"/>
					</panel>
					<commandItem name="p_increase_indent" shortcut="{IndentShortcut}" caption=""/>
					<commandItem name="p_decrease_indent" shortcut="{OutdentShortcut}" caption=""/>
					<separator line="true"/>
					<panel type="subgroup" name="bullet_list" desc="{BulletList}" shortcutArea="false" layout="buttonGrid" dataSize="160">
						<commandItem name="p_menu_bullet_list" captionVisible="false"/>
					</panel>
					<panel type="subgroup" name="number_list" desc="{NumberList}" shortcutArea="false" layout="buttonGrid" dataSize="160">
						<commandItem name="p_menu_number_list" captionVisible="false"/>
					</panel>
					<panel type="subgroup" name="multilevel_list" desc="{MultilevelList}" shortcutArea="false" layout="buttonGrid" dataSize="160">
						<commandItem name="p_menu_multilevel_list" captionVisible="false"/>
					</panel>
					<separator line="true"/>
					<panel type="subgroup" name="paragraph_style" desc="{ParagraphStyle}">
						<commandItem name="p_menu_p_style" icon="false" captionVisible="false"/>
					</panel>
				</panel>
				<panel name="page" type="title" desc="{Page}">
					<button name="d_page_setup" desc="{PageSetup}" uiCommand="show" uiValue="dialog_page_setup" shortcut="{PageSetupShortcut}"/>
					<separator line="true"/>
					<commandItem name="e_header" caption=""/>
					<commandItem name="e_footer" caption=""/>
					<panel type="subgroup" name="page_number" desc="{PageNumber}" shortcutArea="false">
						<commandItem name="p_menu_page_number" captionVisible="false"/>
					</panel>
					<separator line="true"/>
					<commandItem name="p_page_break" shortcut="{PageBreakShortcut}" caption=""/>
				</panel>
				<panel name="guide" type="inline" childVAlign="center">
					<panel childHAlign="right">
						<panel name="message" type="inline" textAlign="right" margin="0 32 0 0" updateDesc="true"/>
						<button name="shortcut" caption="{ShortCutInfo}" uiCommand="show" uiValue="dialog_shortcut_info" margin="0 0 0 0"/>
					</panel>
				</panel>
			</panel>
		</titleBar>
		<menuBar control="true">
			<commandItem name="d_save" size="medium" desc=""/>
			<separator line="true" direction="vertical"/>
			<commandItem name="e_select" size="medium" desc="" margin="6 0 6 7"/>
			<button name="find_replace" uiCommand="show" uiValue="dialog_find_replace" caption="{FindReplace}" size="medium"/>
			<separator line="true" direction="vertical"/>
			<button name="insert_shape" uiCommand="show" uiValue="dialog_insert_shape" caption="{Shape}" size="medium"/>
			<button name="insert_image" uiCommand="show" uiValue="dialog_insert_image" caption="{Image}" size="medium"/>
			<button name="insert_table" uiCommand="show" uiValue="dialog_insert_table" caption="{Table}" size="medium"/>
			<commandItem name="s_insert_textbox" size="medium" desc=""/>
			<separator line="true" direction="vertical"/>
			<commandItem name="e_foot_note" size="medium" desc=""/>
			<commandItem name="e_end_note" size="medium" desc=""/>
			<button name="bookmark" uiCommand="show" uiValue="dialog_insert_bookmark" caption="{Bookmark}" size="medium"/>
			<button name="hyperlink" uiCommand="show" uiValue="dialog_insert_hyperlink" caption="{Hyperlink}" size="medium"/>
			<button name="symbols" uiCommand="show" uiValue="dialog_insert_symbols" caption="{Symbol}" size="medium"/>
			<separator line="true" direction="vertical"/>
			<button name="d_page_setup" uiCommand="show" uiValue="dialog_page_setup" caption="{PageSetup}" size="medium"/>
			<commandItem name="p_page_break" size="medium" desc=""/>
			<separator line="true" direction="vertical"/>
			<button name="show_ruler" uiCommand="toggle" uiValue="e_ruler" caption="{HorizontalRuler}" size="medium"/>
			<button name="comment" uiCommand="toggle" uiValue="commentBar" caption="{ShowComments}" size="medium" disable="true"/>
			<button name="taskpane" size="medium" uiCommand="fold" uiValue="sideBar" caption="{Taskpane}"/>
		</menuBar>
		<styleBar>
			<commandItem name="e_undo" desc=""/>
			<commandItem name="e_redo" desc=""/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="p_style"/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="font_name_combo" size="medium"/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="font_size" size="small"/>
			<separator direction="vertical" line="true" />
			<commandItem name="font_color" margin="0 0 0 -8"/>
			<commandItem name="font_highlight_color" desc=""/>
			<separator direction="vertical" line="true" />
			<commandItem name="bold" desc=""/>
			<commandItem name="italic" desc=""/>
			<commandItem name="underline" desc=""/>
			<commandItem name="strikethrough" desc=""/>
			<separator direction="vertical" line="true" />
			<commandItem name="text_script" descVisible="false"/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="letter_spacing"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="p_align" descVisible="false"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="p_decrease_indent" desc=""/>
			<commandItem name="p_increase_indent" desc=""/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="p_line_spacing" size="medium"/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="p_list"/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="p_before"/>
			<separator direction="vertical" line="true" margin="0 -8 0 0"/>
			<commandItem name="p_after"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="e_format_copy" desc=""/>
			<commandItem name="e_format_remove" desc=""/>
			<separator direction="vertical" line="true"/>
		</styleBar>
		<statusBar>
			<panel>
				<panel type="inline" name="zoom" width="fit" childVAlign="center">
					<panel>
						<commandItem name="e_view_scale" size="medium"/>
						<commandItem name="e_zoom" descVisible="false"/>
					</panel>
				</panel>
				<panel type="inline" name="temp_area"/>
				<panel type="inline" name="ui_option_area" width="fit" hide="true">
					<button name="toggle_ui_option_view" caption="{FoldUnfold}" uiCommand="toggle_ui_option_view" uiValue="" margin="0 10 0 0"/>
				</panel>
				<panel type="inline" name="section_property_wrap" width="fit">
					<panel name="section_property" childHAlign="right">
						<panel name="cur_page" type="inline" width="fit" updateDesc="true"/>
						<panel type="inline" desc="/" width="fit" margin="0 4 0 4"/>
						<panel name="total_page" type="inline" width="fit" updateDesc="true"/>
						<panel type="inline" desc="{Page}" width="fit" margin="0 0 0 4"/>
						<separator line="true" direction="vertical"/>
						<panel type="inline" desc="{Section}" width="fit"/>
						<panel type="inline" desc=":" width="fit" margin="0 3 0 3"/>
						<panel name="section" type="inline" width="fit" margin="0 0 0 4" updateDesc="true"/>
					</panel>
				</panel>
			</panel>
		</statusBar>
		<translate control="true">
			<panel name="trans_content_wrap">
				<panel type="inline">
					<panel type="title" desc="{Translate}" border="underline">
						<commandItem name="e_trans_lang"/>
						<separator line="true" margin="0 0 12 0"/>
						<commandItem name="e_trans_result"/>
					</panel>
				</panel>
				<panel type="inline" name="trans_resize" width="17"/>
			</panel>
		</translate>
		<commentBar control="true">
			<panel name="comments_view">
				<panel name="category_comment" desc="{Comments}"/>
				<commandItem name="e_comment_item"/>
			</panel>
		</commentBar>
		<sideBar control="true">
			<panel name="category" updateDesc="true"/>
			<panel customData="scroll-content:side_bar">
				<!-- 정렬 -->
				<panel name="align" type="title" desc="{ShapeAlign}" border="underline" control="true">
					<panel>
						<separator desc="{ShapeOrder}"/>
						<commandItem name="s_order" size="medium" descVisible="false" margin="0 9 0 0"/>
						<separator desc="{ShapeWrapText}"/>
						<commandItem name="s_text_wrapping" size="medium" descVisible="false" margin="0 9 0 0"/>
					</panel>
				</panel>
				<!-- 셀 배경 및 테두리 -->
				<panel name="background_and_border" type="title" desc="{HighlightAndBorder}" border="underline" control="true">
					<panel>
						<commandItem name="c_background_and_border"/>
					</panel>
				</panel>
				<!-- 그림 -->
				<panel name="image_properties" type="title" desc="{Image}" border="underline" control="true">
					<panel>
						<separator desc="{ImageLine}"/>
						<panel type="inline" margin="0 18 0 0">
							<commandItem name="i_border_style"/>
							<commandItem name="i_border_color" margin="0 0 0 0"/>
						</panel>
						<panel type="inline">
							<commandItem name="i_border_width"/>
						</panel>
					</panel>
				</panel>
				<!-- 도형 -->
				<panel name="shape_properties" type="title" desc="{Shape}" border="underline" control="true">
					<panel>
						<!-- 채우기 -->
						<separator desc="{ShapeFill}"/>
						<commandItem name="s_bgcolor" margin="0 18 0 0"/>
						<commandItem name="s_bgcolor_opacity"/>
						<separator line="true"/>
						<!-- 선 -->
						<separator desc="{ShapeLine}"/>
						<panel type="inline" margin="0 18 0 0">
							<commandItem name="s_border_style"/>
							<commandItem name="s_border_color" margin="0 0 0 0"/>
						</panel>
						<panel type="inline">
							<commandItem name="s_border_width"/>
							<commandItem name="s_border_opacity" margin="0 0 0 0"/>
						</panel>
					</panel>
				</panel>
				<!-- 이미지 크기 및 위치 -->
				<panel name="image_size_and_position" type="title" desc="{ImageSizeAndPosition}" border="underline" control="true">
					<panel>
						<separator desc="{ImageSize}"/>
						<commandItem name="i_size" margin="0 0 12 0"/>
						<separator line="true"/>
						<commandItem name="i_position"/>
					</panel>
				</panel>
				<!-- 도형 크기 및 위치 -->
				<panel name="shape_size_and_position" type="title" desc="{ImageSizeAndPosition}" border="underline" control="true">
					<panel>
						<separator desc="{ImageSize}"/>
						<commandItem name="s_size"/>
						<separator line="true" margin="12 0 0 0"/>
						<commandItem name="s_position"/>
					</panel>
				</panel>
				<!-- 표 -->
				<panel name="table_properties" type="title" desc="{Table}" border="underline" control="true">
					<panel>
						<!-- 행/열 -->
						<separator desc="{RowAndColumn}"/>
						<panel>
							<panelGroupItem name="insert_row_col"/>
							<separator direction="vertical" line="true"/>
							<panel type="inline" childVAlign="center" childHAlign="center">
								<commandItem name="t_delete_row" desc="" size="medium" margin="0 9 0 0"/>
								<commandItem name="t_delete_col" desc="" size="medium" margin="0 9 0 0"/>
							</panel>
						</panel>
						<!-- 셀 -->
						<separator line="true" margin="12 0 0 0"/>
						<separator desc="{Cell}"/>
						<panel>
							<commandItem name="c_merge" desc="" size="medium" margin="0 9 0 0"/>
							<commandItem name="c_width_distribute" desc="" size="medium" margin="0 9 0 0"/>
							<commandItem name="c_height_distribute" desc="" size="medium" margin="0 9 0 0"/>
						</panel>
						<!-- 표 삭제 -->
						<separator desc="{DeleteTable}"/>
						<panel>
							<commandItem name="t_delete" desc="" size="medium"/>
						</panel>
						<!-- 정렬 -->
						<separator line="true"/>
						<panel name="table_properties_align" margin="18 0 0 0">
							<panel type="inline" name="table_align" margin="0 18 0 0">
								<separator desc="{TableAlign}"/>
								<commandItem name="t_align" descVisible="false"/>
								<commandItem name="t_margin_left"/>
							</panel>
							<panel type="inline" name="table_text_align">
								<separator desc="{TableTextAlign}"/>
								<commandItem name="c_valign"/>
							</panel>
						</panel>
						<separator line="true"/>
						<separator desc="{CellPadding}"/>
						<panel>
							<commandItem name="c_padding"/>
						</panel>
					</panel>
				</panel>
				<!-- 텍스트 박스 속성 -->
				<panel name="text_box_properties" type="title" desc="{TextboxPr}" border="underline" control="true">
					<panel>
						<separator desc="{TextboxPadding}"/>
						<commandItem name="s_textbox_padding"/>
						<separator line="true" margin="12 0 0 0"/>
						<separator desc="{TableTextAlign}"/>
						<commandItem name="s_valign" margin="0 9 0 0" descVisible="false"/>
					</panel>
				</panel>
				<!-- 비어있는 사이드바 메세지 -->
				<panel name="unselected_object" type="title">
					<panel desc="{NoObjectTaskPane}" childHAlign="center" childVAlign="center"/>
				</panel>
				<!-- 관전모드일 때 사이드바 메세지 -->
				<panelGroupItem name="watcher_mode_message"/>
			</panel>
			<!-- 사용자 리스트 -->
			<panelGroupItem name="user_list"/>
		</sideBar>
		<contextMenu>
			<commandItem name="e_cut" shortcut="{CutShortcut}" caption=""/>
			<commandItem name="e_copy" shortcut="{CopyShortcut}" caption=""/>
			<commandItem name="e_paste" shortcut="{PasteShortcut}" caption=""/>
			<separator line="true"/>
			<commandItem name="s_add_text" caption=""/>
			<commandItem name="s_edit_text" caption=""/>
			<separator line="true"/>
			<commandItem name="c_merge" caption=""/>
			<commandItem name="c_unmerge" caption=""/>
			<panel type="subgroup" name="insert_row" desc="{InsertRow}" shortcutArea="false">
				<commandItem name="t_insert_row_above" caption=""/>
				<commandItem name="t_insert_row_below" caption=""/>
			</panel>
			<panel type="subgroup" name="insert_col" desc="{InsertColumn}" shortcutArea="false">
				<commandItem name="t_insert_col_left" caption=""/>
				<commandItem name="t_insert_col_right" caption=""/>
			</panel>
			<commandItem name="c_height_distribute" caption=""/>
			<commandItem name="c_width_distribute" caption=""/>
			<panel type="subgroup" name="delete_table" desc="{DeleteTable}" shortcutArea="false">
				<commandItem name="t_delete_row" caption=""/>
				<commandItem name="t_delete_col" caption=""/>
				<commandItem name="t_delete" caption=""/>
			</panel>
			<panel type="subgroup" name="select_table" desc="{TableSelect}" shortcutArea="false">
				<commandItem name="c_select_cell" caption=""/>
				<commandItem name="t_select_table" caption=""/>
			</panel>
			<separator line="true"/>
			<button name="hyperlink" uiCommand="show" uiValue="dialog_insert_hyperlink" shortcut="{HyperlinkShortcut}" desc="{Hyperlink}"/>
			<commandItem name="e_open_link" caption=""/>
			<button name="edit_link" uiCommand="show" uiValue="dialog_insert_hyperlink" desc="{EditLink}"/>
			<commandItem name="e_delete_link" caption=""/>
			<separator line="true"/>
			<commandItem name="e_delete_field" caption="" icon="false"/>
			<separator line="true"/>
			<commandItem name="e_foot_note" shortcut="{FootnoteShortcut}" caption=""/>
			<commandItem name="e_end_note" shortcut="{EndnoteShortcut}" caption=""/>
			<separator line="true"/>
			<commandItem name="e_format_copy" shortcut="{CopyFormatShortcut}" caption=""/>
			<commandItem name="e_format_remove" shortcut="{RemoveFormatShortcut}" caption=""/>
			<commandItem name="p_start_new_list" icon="false" caption=""/>
		</contextMenu>
		<modalDialog>
			<panelGroupItem name="dialog_insert_image"/>
			<panelGroupItem name="dialog_insert_hyperlink"/>
			<panelGroupItem name="dialog_insert_shape"/>
			<panelGroupItem name="dialog_insert_table"/>
			<panelGroupItem name="dialog_insert_symbols"/>
			<panelGroupItem name="dialog_alert"/>
			<panelGroupItem name="dialog_error"/>
			<panelGroupItem name="dialog_confirm"/>
			<panelGroupItem name="dialog_print"/>
			<panelGroupItem name="dialog_download"/>
			<panelGroupItem name="dialog_rename"/>
			<panelGroupItem name="dialog_document_info"/>
			<panelGroupItem name="dialog_shortcut_info"/>
			<panelGroupItem name="dialog_save_as"/>
			<panelGroupItem name="dialog_viewer"/>
			<!-- 북마크 -->
			<dialog name="dialog_insert_bookmark" control="true" width="400">
				<panel type="title" desc="{Bookmark}" border="underline">
					<separator desc="{BookmarkInsert}"/>
					<commandItem name="e_insert_bookmark"/>
					<panel name="bookmark_error_message_box" updateDesc="true"/>
					<separator line="true"/>
					<separator desc="{BookmarkList}"/>
					<commandItem name="e_bookmark_list"/>
				</panel>
			</dialog>
			<!-- 페이지 설정 -->
			<dialog name="dialog_page_setup" control="true">
				<panel type="title" desc="{PageSetup}" border="underline">
					<separator desc="{PageType}"/>
					<commandItem name="d_paper_type"/>
					<separator line="true" margin="12 0 18 0"/>
					<panel>
						<panel type="inline" margin="0 18 0 0">
							<separator desc="{PageDirection}"/>
							<commandItem name="d_orientation"/>
						</panel>
						<panel type="inline">
							<separator desc="{BackgroundColor}"/>
							<commandItem name="d_bgcolor"/>
						</panel>
					</panel>
					<separator line="true" margin="12 0 0 0"/>
					<separator desc="{PageMargin}"/>
					<commandItem name="d_paper_margin"/>
				</panel>
			</dialog>
		</modalDialog>
		<modelessDialog>
			<!-- 찾기/바꾸기 -->
			<dialog name="dialog_find_replace" control="true" width="400">
				<panel name="dialog_title_find_replace" type="title" desc="{FindReplace}" border="underline" updateDesc="true">
					<commandItem name="e_find_replace"/>
				</panel>
			</dialog>
		</modelessDialog>
		<toolBox>
			<tool name="tool_tooltip">
				<panelGroupItem name="tool_tooltip"/>
			</tool>
			<tool name="tool_loading_progress">
				<panelGroupItem name="tool_loading_progress"/>
			</tool>
			<tool name="tool_toast_message">
				<panelGroupItem name="tool_toast_message"/>
			</tool>
			<tool name="tool_screen">
				<panelGroupItem name="tool_screen"/>
			</tool>
			<tool name="tool_quick_menu">
				<panelGroupItem name="quick_menu"/>
			</tool>
		</toolBox>
	</frame>
	<commands>
		<command name="d_paper_type">
			<dropdown type="text" icon="false" size="full" nonExec="true" valueKey="d_paper_type" border="all">
				<dataSetItem name="paper_type" icon="false"/>
			</dropdown>
			<panel>
				<panel type="inline" margin="0 9 0 0">
					<input name="d_width" unit="cm" spinner="true" caption="{ImageSizeWidth}" nonExec="true" valueKey="d_width" border="underline" textIcon="W"/>
				</panel>
				<panel type="inline" margin="0 0 0 9">
