			</panel>
			<panel>
				<checkbox desc="{FrgMatchToCase}" caption="{FrgMatchToCase}" nonExec="true" valueKey="find_case_sensitive"/>
			</panel>
			<!-- 검색 결과 있는 경우 -->
			<panel name="found">
				<panel childHAlign="center" childVAlign="center">
					<button name="nav_back" size="small" valueKey="find_navigation" value="back" margin="0 4 0 0"/>
					<button name="nav_backward" size="small" valueKey="find_navigation" value="backward" margin="0 10 0 0"/>
