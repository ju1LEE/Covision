				<separator line="true"/>
				<commandItem name="s_align_even_horizontal" caption="" disable="true"/>
				<commandItem name="s_align_even_vertical" caption="" disable="true"/>
			</panel>
			<separator line="true"/>
			<commandItem name="i_crop" caption="" disable="true"/>
			<separator line="true"/>
			<button name="hyperlink" uiCommand="show" uiValue="dialog_insert_hyperlink" desc="{Hyperlink}" disable="true"/>
			<commandItem name="e_delete_link" caption=""/>
			<commandItem name="t_insert_col_left" caption=""/>
			<commandItem name="t_insert_col_right" caption=""/>
			<separator line="true"/>
			<commandItem name="t_insert_row_above" caption=""/>
