			<panelGroupItem name="dialog_insert_image"/>
			<panelGroupItem name="dialog_insert_shape"/>
			<panelGroupItem name="dialog_insert_table"/>
			<panelGroupItem name="dialog_insert_symbols"/>
			<panelGroupItem name="dialog_alert"/>
			<panelGroupItem name="dialog_error"/>
