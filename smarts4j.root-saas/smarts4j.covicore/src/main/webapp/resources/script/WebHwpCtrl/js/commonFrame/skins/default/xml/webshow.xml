			<data desc="{Russian}" value="Russian"/>
		</dataSet>
	</dataSets>
	<color>
		<titleBar>
			<titlePanel bgColorHover="#f7eae1" borderColorHover="#d5a39c" textColorHover="#b54637"
						bgColorOn="#ea5a47" borderColorOn="#d25140" textColorOn="#ffffff"/>
			<resultButtons bgColorHover="#f7eae1" borderColorHover="#d5a39c" textColorHover="#b54637"/>
		</titleBar>
	</color>
	<viewMode>
		<sideBar delete="true"/>
		<menuBar delete="true"/>
		<styleBar delete="true"/>
		<titleBar>
			<panel name="message" delete="true"/>
			<panel name="bottom_title" overwrite="true">
				<panel name="file" type="title" desc="{File}">
					<commandItem name="d_download" caption="" disable="true"/>
					<commandItem name="d_pdf_download" caption="" disable="true"/>
					<separator line="true"/>
					<commandItem name="d_print" caption="" shortcut="{PrintShortcut}" disable="true"/>
					<separator line="true"/>
					<button name="d_info" uiCommand="show" uiValue="dialog_document_info" desc="{DocumentInfo}"/>
				</panel>
				<panel name="view" type="title" desc="{View}">
					<panel type="subgroup" name="zoom" desc="{Zoom}" shortcutArea="false">
						<commandItem name="e_menu_view_scale"/>
					</panel>
				</panel>
				<panel name="slide" type="title" desc="{main_menu_view_slide_show}">
					<commandItem name="e_show_mode_start" shortcut="{SlideShowShortcut}" caption="" disable="true"/>
					<commandItem name="e_show_mode" shortcut="{SlideShowFromCurrentSlideShortcut}" caption="" disable="true"/>
				</panel>
			</panel>
		</titleBar>
		<thumbnail>
			<panel name="new_slide" delete="true"/>
		</thumbnail>
		<statusBar>
			<insertBefore name="e_show_mode">
				<commandItem name="e_show_mode_start" caption="" disable="true"/>
			</insertBefore>
		</statusBar>
		<button name="shortcut" delete="true"/>
		<button name="e_help" delete="true"/>
	</viewMode>
</webOffice>
