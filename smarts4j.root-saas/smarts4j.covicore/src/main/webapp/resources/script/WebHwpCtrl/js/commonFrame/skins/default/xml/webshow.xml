			<separator line="true"/>
			<commandItem name="t_delete_col" caption=""/>
			<commandItem name="t_delete_row" caption=""/>
			<separator line="true"/>
			<commandItem name="c_merge" caption="" disable="true"/>
			<commandItem name="c_unmerge" caption="" disable="true"/>
			<separator line="true"/>
			<commandItem name="u_search_doc_context" caption=""/>
		</contextMenu>
