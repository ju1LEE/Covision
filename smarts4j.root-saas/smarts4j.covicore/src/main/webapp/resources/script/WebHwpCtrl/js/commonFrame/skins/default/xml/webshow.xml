<webOffice>
	<frame>
		<titleBar>
			<panel name="top_title">
				<panel type="inline" name="bi_show" width="fit"/>
				<panel type="inline" name="guide">
					<panel childVAlign="center">
						<panel name="file_name" type="inline" textAlign="right" updateDesc="true"/>
						<panel name="message" type="inline" margin="0 0 0 10" updateDesc="true"/>
						<button name="shortcut" caption="{ShortCutInfo}" uiCommand="show" uiValue="dialog_shortcut_info"/>
						<separator line="true" direction="vertical" margin="6 -4 6 -5"/>
						<button name="e_help" caption="{Help}" onLink="{HELP_URL_WEBSHOW}"/>
					</panel>
				</panel>
			</panel>
			<panel name="bottom_title">
				<panel name="file" type="title" desc="{File}">
					<commandItem name="d_save" caption="" disable="true"/>
					<button name="d_save_as_button" uiCommand="show" uiValue="dialog_save_as" desc="{DocumentSaveAs}"/>
					<separator line="true"/>
					<commandItem name="d_download" caption="" disable="true"/>
					<commandItem name="d_pdf_download" caption="" disable="true"/>
					<separator line="true"/>
					<commandItem name="d_print" caption="" shortcut="{PrintShortcut}" disable="true"/>
					<separator line="true"/>
					<button name="d_info" uiCommand="show" uiValue="dialog_document_info" desc="{DocumentInfo}"/>
				</panel>
				<separator line="true" direction="vertical" margin="6 -1 6 7"/>
				<panel name="edit" type="title" desc="{Edit}">
					<commandItem name="e_undo" shortcut="{UndoShortcut}" caption=""/>
					<commandItem name="e_redo" shortcut="{RedoShortcut}" caption=""/>
					<separator line="true"/>
					<commandItem name="e_cut" shortcut="{CutShortcut}" caption="" disable="true"/>
					<commandItem name="e_copy" shortcut="{CopyShortcut}" caption="" disable="true"/>
					<commandItem name="e_paste" shortcut="{PasteShortcut}" caption="" disable="true"/>
					<commandItem name="e_duplicate_obj" shortcut="{DuplicateObjShortcut}" caption="" disable="true"/>
					<separator line="true"/>
					<panel type="subgroup" name="slide_layout_menu" desc="{ChangeLayout}" layout="buttonGrid" shortcutArea="false" dataSize="422">
						<commandItem name="e_slide_change"/>
					</panel>
					<commandItem name="e_reset_slide" caption="" disable="true"/>
					<separator line="true"/>
					<commandItem name="e_delete_slide" caption="" disable="true"/>
					<separator line="true"/>
					<commandItem name="e_select_all" shortcut="{SelectAllShortcut}" caption="" disable="true"/>
				</panel>
				<panel name="view" type="title" desc="{View}">
					<panel type="subgroup" name="zoom" desc="{Zoom}" shortcutArea="false">
						<commandItem name="e_menu_view_scale"/>
					</panel>
					<separator line="true"/>
					<commandItem name="e_show_mode_start" shortcut="{SlideShowShortcut}" desc="{main_menu_view_slide_show}" caption="" disable="true"/>
					<separator line="true"/>
					<!-- 도구 상자 -->
					<!--<panel type="subgroup" name="tool_box" icon="false" desc="{Toolbox}" shortcutArea="false">-->
						<!--<button name="toolbox_standard" uiCommand="fold" uiValue="menuBar" desc="{ToolboxStandard}" icon="false"/>-->
						<!--<button name="toolbox_formatting" uiCommand="toggle" uiValue="style_bar" desc="{ToolboxFormatting}" icon="false"/>-->
					<!--</panel>-->
					<button name="taskpane" uiCommand="fold" uiValue="sideBar" desc="{Taskpane}" disable="true"/>
					<!-- 문서 창 -->
					<!--<panel type="subgroup" name="document_window" icon="false" desc="{DocumentWindow}" shortcutArea="false">-->
						<!-- 상황 선 -->
						<!--<button name="statusbar" uiCommand="toggle" uiValue="status_bar" desc="{StatusBar}" icon="false"/>-->
					<!--</panel>-->
				</panel>
				<panel name="insert" type="title" desc="{Insert}">
					<commandItem name="e_new_slide" caption="" disable="true"/>
					<commandItem name="e_duplicate_slide" caption="" disable="true"/>
					<separator line="true"/>
					<button name="insert_shape" desc="{Shape}" uiCommand="show" uiValue="dialog_insert_shape" disable="true"/>
					<button name="insert_image" desc="{Image}" uiCommand="show" uiValue="dialog_insert_image" disable="true"/>
					<button name="insert_table" desc="{Table}" uiCommand="show" uiValue="dialog_insert_table" disable="true"/>
					<commandItem name="s_insert_textbox" caption="" disable="true"/>
					<button name="symbols" desc="{Symbol}" uiCommand="show" uiValue="dialog_insert_symbols" shortcut="{SymbolShortcut}" disable="true"/>
					<separator line="true"/>
					<button name="header_footer" desc="{HeaderFooter}" uiCommand="show" uiValue="dialog_header_footer" disable="true"/>
					<button name="date_time" desc="{DateTime}" uiCommand="show" uiValue="dialog_header_footer" disable="true"/>
					<button name="slide_number" desc="{SlideNumber}" uiCommand="show" uiValue="dialog_header_footer" disable="true"/>
					<separator line="true"/>
					<button name="hyperlink" desc="{Hyperlink}" uiCommand="show" uiValue="dialog_insert_hyperlink" disable="true"/>
				</panel>
				<panel name="format" type="title" desc="{Format}">
					<panel type="subgroup" name="font_name" desc="{FontName}" icon="false" shortcutArea="false">
						<commandItem name="menu_font_name" icon="false" captionVisible="false" disable="true"/>
					</panel>
					<commandItem name="bold" caption="" shortcut="{BoldShortcut}" disable="true"/>
					<commandItem name="italic" caption="" shortcut="{ItalicShortcut}" disable="true"/>
					<commandItem name="underline" caption="" shortcut="{UnderlineShortcut}" disable="true"/>
					<commandItem name="strikethrough" caption="" disable="true"/>
					<commandItem name="text_script" captionVisible="false" disable="true"/>
					<separator line="true"/>
					<panel type="subgroup" name="p_align" desc="{Alignment}">
						<commandItem name="p_align" caption="" disable="true"/>
					</panel>
					<panel type="subgroup" name="line_spacing" desc="{Lineheight}" shortcutArea="false">
						<commandItem name="p_menu_line_spacing" icon="false" captionVisible="false" disable="true"/>
					</panel>
					<commandItem name="p_increase_indent" caption="" disable="true"/>
					<commandItem name="p_decrease_indent" caption="" disable="true"/>
					<separator line="true"/>
					<panel type="subgroup" name="bullet_list" desc="{BulletList}" shortcutArea="false" layout="buttonGrid" dataSize="186">
						<commandItem name="p_menu_bullet_list" captionVisible="false" disable="true"/>
					</panel>
					<panel type="subgroup" name="number_list" desc="{NumberList}" shortcutArea="false" layout="buttonGrid" dataSize="186">
						<commandItem name="p_menu_number_list" captionVisible="false" disable="true"/>
					</panel>
					<panel type="subgroup" name="slide_layout_menu" desc="{property_update_slide_layout_group}" layout="buttonGrid" dataSize="422">
						<commandItem name="e_slide_change"/>
					</panel>
				</panel>
				<panel name="slide" type="title" desc="{main_menu_view_slide_show}">
					<commandItem name="e_show_mode_start" shortcut="{SlideShowShortcut}" caption="" disable="true"/>
					<commandItem name="e_show_mode" shortcut="{SlideShowFromCurrentSlideShortcut}" caption="" disable="true"/>
					<separator line="true"/>
					<commandItem name="e_hide_slide" caption="" disable="true"/>
					<commandItem name="e_show_slide" caption="" disable="true"/>
				</panel>
				<panel name="tools" type="title" desc="{Tools}">
					<button name="storage_search_doc" uiCommand="show" uiValue="dialog_search_doc" desc="{SearchInformation}"/>
				</panel>
			</panel>
		</titleBar>
		<menuBar control="true">
			<commandItem name="d_save" direction="vertical" size="large" disable="true"/>
			<commandItem name="d_download" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<commandItem name="e_select_all" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<commandItem name="e_new_slide" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<button name="insert_shape" caption="{Shape}" desc="{Shape}" uiCommand="show" uiValue="dialog_insert_shape" direction="vertical" size="large" disable="true"/>
			<button name="insert_image" caption="{Image}" desc="{Image}" uiCommand="show" uiValue="dialog_insert_image" direction="vertical" size="large" disable="true"/>
			<button name="insert_table" caption="{Table}" desc="{Table}" uiCommand="show" uiValue="dialog_insert_table" direction="vertical" size="large" disable="true"/>
			<commandItem name="s_insert_textbox" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<button name="hyperlink" caption="{Hyperlink}" desc="{Hyperlink}" uiCommand="show" uiValue="dialog_insert_hyperlink" direction="vertical" size="large" disable="true"/>
			<button name="symbols" caption="{Symbol}" desc="{Symbol}" uiCommand="show" uiValue="dialog_insert_symbols" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<button name="header_footer" caption="{HeaderFooter}" desc="{HeaderFooter}" uiCommand="show" uiValue="dialog_header_footer" direction="vertical" size="large" disable="true"/>
			<button name="date_time" caption="{DateTime}" desc="{DateTime}" uiCommand="show" uiValue="dialog_header_footer" direction="vertical" size="large" disable="true"/>
			<button name="slide_number" caption="{SlideNumber}" desc="{SlideNumber}" uiCommand="show" uiValue="dialog_header_footer" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<button name="slide_change" caption="{property_update_slide_layout_group}" desc="{property_update_slide_layout_group}" uiCommand="show" uiValue="dialog_layout" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<commandItem name="e_show_mode_start" direction="vertical" size="large" disable="true"/>
			<commandItem name="e_show_mode" direction="vertical" size="large" disable="true"/>
			<separator line="true" direction="vertical"/>
			<button name="taskpane" direction="vertical" size="large" uiCommand="fold" uiValue="sideBar" caption="{Taskpane}" desc="{Taskpane}" disable="true"/>
			<separator line="true" direction="vertical"/>
			<button name="storage_search_doc" direction="vertical" size="large" uiCommand="show" uiValue="dialog_search_doc" caption="{SearchInformation}" desc="{SearchInformation}" disable="true"/>
		</menuBar>
		<styleBar>
			<commandItem name="e_undo" desc=""/>
			<commandItem name="e_redo" desc=""/>
			<separator direction="vertical" line="true"/>
			<commandItem name="font_name_combo" size="medium" disable="true"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="font_size" size="small" disable="true"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="font_color" disable="true"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="bold" desc="" disable="true"/>
			<commandItem name="italic" desc="" disable="true"/>
			<commandItem name="underline" desc="" disable="true"/>
			<commandItem name="strikethrough" desc="" disable="true"/>
			<separator direction="vertical" line="true" />
			<commandItem name="text_script" descVisible="false" disable="true"/>
			<separator direction="vertical" line="true" />
			<commandItem name="p_align" descVisible="false" disable="true"/>
			<separator direction="vertical" line="true" />
			<commandItem name="p_decrease_indent" desc="" disable="true"/>
			<commandItem name="p_increase_indent" desc="" disable="true"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="p_line_spacing" disable="true"/>
			<separator direction="vertical" line="true"/>
			<commandItem name="p_list" disable="true"/>
			<separator direction="vertical" line="true"/>
		</styleBar>
		<thumbnail>
			<panel name="new_slide" height="36" childHAlign="center" childVAlign="center" border="underline">
				<commandItem name="e_add_slide" disable="true"/>
			</panel>
			<panel name="slide_list">
				<panel name="slide_insert"/>
				<panel name="thumbnail"/>
			</panel>
		</thumbnail>
		<document>
			<panel name="edit_area"/>
			<panelGroupItem name="message_bar"/>
		</document>
		<statusBar>
			<panel>
				<panel type="inline" name="slide_navigation" width="fit">
					<panel childVAlign="center">
						<commandItem name="e_first_slide" desc="" margin="0 8 0 0" disable="true"/>
						<commandItem name="e_previous_slide" desc="" margin="0 8 0 0" disable="true"/>
						<commandItem name="e_next_slide" desc="" margin="0 8 0 0" disable="true"/>
						<commandItem name="e_last_slide" desc="" disable="true"/>
						<separator line="true" direction="vertical"/>
						<panel name="cur_page" type="inline" width="fit" updateDesc="true"/>
						<panel type="inline" desc="/" width="fit" margin="0 4 0 4"/>
						<panel name="total_page" type="inline" width="fit" updateDesc="true"/>
						<panel desc="{status_bar_slide_number}" type="inline" width="fit" margin="0 8 0 4"/>
					</panel>
				</panel>
				<panel type="inline" width="170" childVAlign="center">
					<progress name="slide_progress" textPosition="top"/>
				</panel>
				<panel type="inline"/>
				<panel type="inline" name="ui_option_area" width="fit" hide="true" childVAlign="center">
					<button name="toggle_ui_option_view" caption="{FoldUnfold}" uiCommand="toggle_ui_option_view" uiValue="" margin="0 8 0 8"/>
				</panel>
				<panel type="inline" childHAlign="right" childVAlign="center" width="fit">
					<commandItem name="e_show_mode" desc="" disable="true"/>
					<separator line="true" direction="vertical"/>
					<commandItem name="e_view_scale" size="medium"/>
					<commandItem name="e_zoom" descVisible="false"/>
				</panel>
			</panel>
		</statusBar>
		<sideBar control="true">
			<panel name="category" updateDesc="true"/>
			<panel customData="scroll-content:side_bar">
				<!-- 슬라이드 배경 및 서식 -->
				<panel name="slide_background_properties" type="title" desc="{property_update_slide_background_group}" control="true" border="underline">
					<commandItem name="e_slide_background"/>
					<panel>
						<panel type="inline">
							<commandItem name="e_slide_bgcolor_opacity" border="all" margin="0 0 10 0"/>
						</panel>
						<panel type="inline"/>
					</panel>
					<separator line="true"/>
					<commandItem name="e_slide_hide_graphic" margin="10 0 0 0"/>
				</panel>
				<!-- 표 속성 -->
				<panel name="table_properties" type="title" desc="{Table}" control="true" border="underline">
					<panel>
						<separator desc="{property_update_table_table_style}"/>
						<commandItem name="t_style" size="medium" margin="0 0 10 0" disable="true"/>
						<separator line="true"/>
						<separator desc="{RowAndColumn}"/>
						<panel>
							<panelGroupItem name="insert_row_col"/>
							<separator direction="vertical" line="true" margin="0 4 0 4"/>
							<panel type="inline" childVAlign="center" childHAlign="center">
								<commandItem name="t_delete_row" desc="" size="medium" margin="0 6 0 0" border="all"/>
								<commandItem name="t_delete_col" desc="" size="medium" margin="0 6 0 0" border="all"/>
							</panel>
						</panel>
					</panel>
				</panel>
				<!-- 셀 속성 -->
				<panel name="cell_properties" type="title" desc="{property_update_table_cell_group}" control="true" border="underline">
					<panel>
						<separator desc="{property_update_shape_fill}"/>
						<panel type="inline" margin="0 6 0 0" width="fit">
							<commandItem name="c_bgcolor" border="all" disable="true"/>
						</panel>
						<commandItem name="c_bgcolor_opacity" border="all" margin="0 0 10 0" disable="true"/>
						<separator line="true"/>
						<separator desc="{TableTextAlign}"/>
						<commandItem name="c_valign" border="all" size="medium" margin="0 6 10 0" disable="true"/>
						<separator line="true"/>
						<separator desc="{main_menu_table_merge_cells}"/>
						<commandItem name="c_merge" desc="" size="medium" margin="0 6 0 0" border="all" disable="true"/>
						<commandItem name="c_unmerge" desc="" size="medium" margin="0 6 0 0" border="all" disable="true"/>
					</panel>
				</panel>
				<!-- 테두리 -->
				<panel name="border" type="title" desc="{Border}" control="true" border="underline">
					<panel>
						<separator desc="{property_update_shape_line}"/>
						<commandItem name="c_border" descVisible="false"/>
					</panel>
				</panel>
				<!-- 텍스트 박스 -->
				<panel name="text_box_properties" type="title" desc="{property_update_textbox_group}" control="true" border="underline">
					<panel>
						<separator desc="{main_menu_format_vertical_alignment}"/>
						<commandItem name="s_valign" margin="0 6 0 0" border="all" size="medium" descVisible="false" disable="true"/>
						<separator desc="{main_menu_format_autofit}"/>
						<commandItem name="s_autofit" disable="true"/>
					</panel>
				</panel>
				<!-- 도형 속성 -->
				<panel name="shape_properties" type="title" desc="{property_update_shape_group}" control="true" border="underline">
					<panel>
						<separator desc="{property_update_shape_fill}"/>
						<panel type="inline" margin="0 6 0 0" width="fit">
							<commandItem name="s_bgcolor" border="all" disable="true"/>
						</panel>
						<commandItem name="s_bgcolor_opacity" border="all" margin="0 0 10 0" disable="true"/>
						<separator line="true"/>
						<separator desc="{property_update_shape_line}"/>
						<panel>
							<panel type="inline" margin="0 6 0 0" width="110">
								<commandItem name="s_border_style" border="all" disable="true"/>
								<commandItem name="s_border_color" border="all" margin="0 0 10 0" disable="true"/>
							</panel>
							<panel type="inline">
								<commandItem name="s_border_width" disable="true"/>
								<commandItem name="s_border_opacity" border="all" margin="0 0 10 0" disable="true"/>
							</panel>
						</panel>
						<separator line="true"/>
						<panel>
							<panel type="inline">
								<separator desc="{ShapeEndLine}"/>
								<commandItem name="s_stroke_end_cap" descVisible="false" border="all" margin="0 6 10 0" disable="true"/>
							</panel>
							<panel type="inline">
								<separator desc="{CornerRounding}"/>
								<commandItem name="s_stroke_join_type" descVisible="false" border="all" margin="0 6 10 0" disable="true"/>
							</panel>
						</panel>
						<separator line="true"/>
						<separator desc="{ArrowType}"/>
						<panel>
							<panel type="inline" margin="0 6 0 0" width="fit">
								<commandItem name="s_stroke_arrow_start_type" descVisible="false" margin="0 0 0 0" border="all" disable="true"/>
							</panel>
							<panel type="inline">
								<commandItem name="s_stroke_arrow_end_type" descVisible="false" margin="0 0 0 0" border="all" disable="true"/>
							</panel>
						</panel>
					</panel>
				</panel>
				<!-- 그림 속성 -->
				<panel name="image_properties" type="title" desc="{ImageProperty}" control="true" border="underline">
					<panel>
						<separator desc="{Crop}"/>
						<commandItem name="i_crop" desc="" size="medium" border="all" margin="0 0 0 0" disable="true"/>
					</panel>
				</panel>
				<!-- 단일 도형 정렬 -->
				<panel name="single_arrange" type="title" desc="{ShapeAlign}" control="true" border="underline">
					<panel>
						<separator desc="{ShapeOrder}"/>
						<commandItem name="s_order" descVisible="false" size="medium" border="all" margin="0 6 10 0" disable="true"/>
						<separator line="true"/>
						<panelGroupItem name="arrangement_align" border="all"/>
					</panel>
				</panel>
				<!-- 다중 도형 정렬 -->
				<panel name="multi_arrange" type="title" desc="{ShapeAlign}" control="true" border="underline">
					<panel>
						<panelGroupItem name="arrangement_align" margin="18 0 0 0"/>
						<separator line="true" margin="10 0 0 0"/>
						<panelGroupItem name="group"/>
					</panel>
				</panel>
				<!-- 그룹 도형 정렬 -->
				<panel name="group_arrange" type="title" desc="{ShapeAlign}" control="true" border="underline">
					<panel>
						<separator desc="{ShapeOrder}"/>
						<commandItem name="s_order" descVisible="false" size="medium" border="all" margin="0 6 10 0" disable="true"/>
						<separator line="true"/>
						<panelGroupItem name="arrangement_align"/>
						<separator line="true" margin="10 0 0 0"/>
						<panelGroupItem name="group"/>
					</panel>
				</panel>
				<!-- 편집 미지원 개체 -->
				<panel name="not_support_object" type="title">
					<panel margin="16 0 0 0">
						<panel desc="{property_not_support_object_message}"/>
					</panel>
				</panel>
				<!-- 관전모드일 때 사이드바 메세지 -->
				<panelGroupItem name="watcher_mode_message"/>
			</panel>
			<!-- 사용자 리스트 -->
			<panelGroupItem name="user_list"/>
		</sideBar>
		<contextMenu>
			<commandItem name="e_new_slide" caption="" disable="true"/>
			<commandItem name="e_delete_slide" caption="" disable="true"/>
			<commandItem name="e_duplicate_slide" caption="" disable="true"/>
