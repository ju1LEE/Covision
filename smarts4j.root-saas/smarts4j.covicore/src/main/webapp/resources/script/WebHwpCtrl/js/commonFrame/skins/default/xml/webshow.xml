				<commandItem name="s_group" caption="" disable="true"/>
				<commandItem name="s_ungroup" caption="" disable="true"/>
			</panel>
			<panel type="subgroup" name="order" desc="{main_menu_arrange_order}" shortcutArea="false" icon="false">
