<script type="text/javascript">
$(function(){					
	setCmEvent();
});

function setCmEvent(){
	$('.tabMenu>li').on('click', function(){
		$('.tabMenu>li').removeClass('active');
		$('.tabContent').removeClass('active');
		$(this).addClass('active');
		$('.tabContent').eq($(this).index()).addClass('active');
	});		
}

</script>